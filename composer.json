{
    "name": "thekavish/laravel-modern-fcm",
    "description": "The package to integrate FCM notifications with image to android and iOS devices",
    "keywords": [
        "thekavish",
        "laravel-modern-fcm"
    ],
    "homepage": "https://github.com/thekavish/laravel-modern-fcm",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kavish Patel",
            "email": "kavish619@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Thekavish\\LaravelModernFcm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thekavish\\LaravelModernFcm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thekavish\\LaravelModernFcm\\LaravelModernFcmServiceProvider"
            ],
            "aliases": {
                "LaravelModernFcm": "Thekavish\\LaravelModernFcm\\LaravelModernFcmFacade"
            }
        }
    }
}
